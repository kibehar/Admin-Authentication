{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "types": ["node", "bcrypt"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
"moduleResolution": "node",
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }